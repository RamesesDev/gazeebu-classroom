<schema>
	<element name="student" tablename="userprofile">
		<field name="objid" primary="true" />
		<field name="firstname" />
		<field name="lastname" />
		<field name="birthdate" />
		<field name="email"/>
		<field name="gender"/>
		<field name="profile"/>
		<complex name="contacts" serializer="default" merge="true"/>
	</element>	
</schema>
